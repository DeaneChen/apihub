{
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "read_timeout": 60,
    "write_timeout": 60
  },
  "database": {
    "type": "sqlite",
    "dsn": "apihub.db",
    "max_conns": 10,
    "max_idle": 5
  },
  "auth": {
    "jwt": {
      "secret": "",
      "access_expiry": 86400000000000,
      "issuer": "apihub"
    },
    "apikey": {
      "secret": ""
    },
    "cache": {
      "default_expiration": 1800000000000,
      "cleanup_interval": 600000000000
    }
  },
  "log": {
    "level": "info",
    "format": "json",
    "path": "logs"
  }
} 